{
  "manifest_version": 2,
 
  "name": "Cards",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",
 
  "content_scripts": [
    {
    "matches":[
      "<all_urls>"],  
    "js": ["contentScript.js"],
    "run_at": "document_idle"
  }
  ],

"browser_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html"
},

"background": {
  "page": "background.html",
  "persistent": false
},
"permissions": [
"clipboardWrite",
"identity"
],
"content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
"oauth2": {
  "client_id": "518804757170-dph2hr8an16ne5ku829dllvf0vd76vtk.apps.googleusercontent.com",
  "scopes":["https://www.googleapis.com/auth/documents"]
},
"key": ""

}